{
  "name": "@ianwremmel/eslint-config",
  "version": "0.0.0-development",
  "description": "Common eslint rules for @ianwremmel's projects",
  "main": "./es2015.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent test:es5 && npm run --silent test:es2015 && npm run --silent test:es2015-imports && npm run --silent test:mocha && npm run --silent test:react && npm run --silent test:script",
    "test:es5": "eslint -c ./es5.js ./test/es5.js",
    "test:es2015": "eslint -c ./es2015.js ./test/es2015.js",
    "test:es2015-imports": "eslint -c ./es2015-imports.js ./test/es2015-imports.js",
    "test:mocha": "eslint -c ./mocha.js ./test/mocha.js",
    "test:react": "eslint -c ./react.js ./test/react.js",
    "test:script": "eslint -c ./script.js ./test/script.js",

    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Ian W. Remmel <design@ianwremmel.com>",
  "license": "MIT",
  "peerDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": ">= 3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianwremmel/eslint-config.git"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "condition-circle": "^1.5.0",
    "eslint": ">= 3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "semantic-release": "^6.3.2"
  },
  "release": {
    "verifyConditions": [
      "condition-circle"
    ]
  },
  "dependencies": {
    "@ciscospark/eslint-config": "^0.7.34"
  }
}
